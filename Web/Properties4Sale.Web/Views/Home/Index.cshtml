           @foreach (var property in this.Model.VisualiseProperties)
           {
               <div class="carousel-item-a intro-item bg-image" style="background-image: url(@property.ImageUrl)">
                   <div class="overlay overlay-a"></div>
                   <div class="intro-content display-table">
                       <div class="table-cell">
                           <div class="container">
                               <div class="row">
                                   <div class="col-lg-8">
                                       <div class="intro-body">
                                           <p class="intro-title-top">
                                               @property.TypeOfPropertyName
                                               <br> @property.Location
                                           </p>
                                           <h1 class="intro-title mb-4">
                                               <span class="color-b"> </span>
                                               <br> @property.Name
                                           </h1>
                                           <p class="intro-subtitle intro-price">
                                               <a asp-controller="Properties" asp-action="ById" asp-route-id="@property.Id"><span class="price-a">Buy | $ @property.Price</span></a>
                                           </p>
                                       </div>
                                   </div>
                               </div>
                           </div>
                       </div>
                   </div>
               </div>
           }

          
       </div>
  </div><!-- End Intro Section -->

